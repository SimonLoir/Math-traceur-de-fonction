{
    "name": "smath",
    "version": "3.18.2",
    "description": "A graphical calculator that works everywhere",
    "main": "app/main.js",
    "scripts": {
        "dev": "webpack",
        "prod": "webpack -p",
        "start": "electron .",
        "dist": "build --windows --ia32 --x64",
        "sw": "node build.sw.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SimonLoir/SMath.git"
    },
    "author": "Simon Loir",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/SimonLoir/SMath/issues"
    },
    "homepage": "https://github.com/SimonLoir/SMath#readme",
    "dependencies": {
        "@types/three": "^0.89.12",
        "ocrad.js": "0.0.1",
        "tesseract.js": "^1.0.10",
        "three-orbit-controls": "^82.1.0"
    },
    "devDependencies": {
        "css-loader": "^0.28.11",
        "electron": "^1.8.8",
        "electron-builder": "^19.56.2",
        "node-gyp": "^3.8.0",
        "node-sass": "^4.9.3",
        "sass-loader": "^6.0.7",
        "style-loader": "^0.20.3",
        "three": "^0.91.0",
        "ts-loader": "^3.5.0",
        "typescript": "^2.9.2",
        "webpack": "^3.12.0"
    },
    "build": {
        "appId": "simonloir.smath",
        "win": {
            "target": "nsis",
            "icon": "images/smath-logo.ico"
        },
        "directories": {
            "output": "builds"
        },
        "files": [
            {
                "filter": "!firebase-bot/**/*"
            }
        ]
    }
}

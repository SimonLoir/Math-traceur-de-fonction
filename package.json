{
  "name": "smath",
  "version": "2.17.12",
  "description": "Traceur de fonctions",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "win-64": "electron-packager . --platefrom=win32 --overwrite --asar --out=releases --icon=logo.ico --electron-version=1.8.1",
    "win-32": "electron-packager . --platefrom=win32 --overwrite --asar --out=releases --icon=logo.ico --electron-version=1.8.1 --arch=ia32",
    "dist": "build --windows --linux --ia32 --x64"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SimonLoir/Math-traceur-de-fonctions.git"
  },
  "author": "Simon Loir",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SimonLoir/Math-traceur-de-fonctions/issues"
  },
  "homepage": "https://github.com/SimonLoir/Math-traceur-de-fonctions#readme",
  "dependencies": {},
  "build": {
    "appId": "simonloir.smath",
    "win": {
      "target": "nsis",
      "icon": "logo.ico"
    }
  },
  "devDependencies": {
    "electron": "^1.8.1",
    "electron-builder": "^19.49.3",
    "electron-packager": "^9.1.0"
  }
}
